config_version=4

_global_script_classes=[ {
"base": "Area2D",
"class": "Bullet",
"language": "GDScript",
"path": "res://Scripts/Bullet.gd"
}, {
"base": "PathFollow2D",
"class": "Enemy",
"language": "GDScript",
"path": "res://Scripts/Enemy.gd"
} ]
_global_script_class_icons={
"Bullet": "",
"Enemy": ""
}

[application]

config/name="BindingTower"
run/main_scene="res://Nodes/Scenes/Menu/TitleScreen.tscn"
